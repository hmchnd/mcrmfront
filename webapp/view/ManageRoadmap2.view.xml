                <items>
                    <IconTabFilter text="Project Management">
                        <VBox
                id="_IDGenVBox"
                class="sapUiSmallMargin"
            >
                <PlanningCalendar
                    id="PC1"
                    startDate="{path: '/startDate'}"
                    rows="{path: '/Phases'}"
                    appointmentsVisualization="Filled"
                    appointmentSelect="onPress"
                    viewKey="Month"
                    appointmentHeight="Large"
                >
                    <toolbarContent>
                        <Title
                            text="Project Management"
                            titleStyle="H4"
                        />
                        <Label text="Logged in as" />
                        <Select
                            id="userRole"
                            change="handleRoleChange"
                            selectedKey="admin"
                            width="230px"
                        >
                            <items>
                                <core:Item
                                    key="admin"
                                    text="Admin"
                                />
                                <core:Item
                                    key="manager"
                                    text="Manager"
                                />
                                <core:Item
                                    key="donna"
                                    text="Donna Moore"
                                />
                            </items>
                        </Select>
                    </toolbarContent>
                    <rows>
                        <PlanningCalendarRow
                            id="_IDGenPlanningCalendarRow"
                            title="{Title}"
                            enableAppointmentsDragAndDrop="{path:'Title', formatter:'.canModifyAppointments'}"
                            enableAppointmentsResize="{path:'Title', formatter:'.canModifyAppointments'}"
                            enableAppointmentsCreate="{path:'Title', formatter:'.canModifyAppointments'}"
                            appointmentDrop="handleAppointmentDrop"
                            appointmentDragEnter="handleAppointmentDragEnter"
                            appointmentResize="handleAppointmentResize"
                            appointmentCreate="handleAppointmentCreate"
                            appointments="{path : 'Tasks', templateShareable: false}"
                        >
                            <appointments>
                                <unified:CalendarAppointment
                                    id="_IDGenCalendarAppointment"
                                    startDate="{start}"
                                    endDate="{end}"
                                    icon="{pic}"
                                    type="{type}"
                                    tentative="{tentative}"
                                >
                                    <unified:customContent>
                                        <VBox>
                                            <Text text="{Title}" />
 
                                            <ProgressIndicator
                                                percentValue="80"
                                                displayValue="80%"
                                                state="Success"
                                            />
                                        </VBox>
                                    </unified:customContent>
                                </unified:CalendarAppointment>
                            </appointments>
                        </PlanningCalendarRow>
                    </rows>
                </PlanningCalendar>
            </VBox>
