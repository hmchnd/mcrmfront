<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
>
    <Dialog
        id="_IDGenDialog3123"
        title="Edit Activity "
    >
        <content>
            <f:Form
                id="FormPage"
                editable="true"
            >
                <f:layout>
                    <f:ResponsiveGridLayout
                        id="ResponsiveGridLayout"
                        labelSpanXL="3"
                        labelSpanL="3"
                        labelSpanM="3"
                        labelSpanS="12"
                        adjustLabelSpan="false"
                        emptySpanXL="4"
                        emptySpanL="4"
                        emptySpanM="4"
                        emptySpanS="0"
                        columnsXL="1"
                        columnsL="1"
                        columnsM="1"
                        singleContainerFullSize="false"
                    />
                </f:layout>
                <f:formContainers>
                    <f:FormContainer>
                        <f:formElements>
                            <f:FormElement label="Title">
                                <f:fields>
                                    <Input
                                        id="TitleInput"
                                        value="Create an Innovation Strategy and a High-Level Road Map"
                                    />
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="Sub Title">
                                <f:fields>
                                    <Input
                                        id="SubtitleInput"
                                        value="Create an Innovation Strategy and a High-Level Road Map"
                                    />
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="Task">
                                <f:fields>
                                    <Input
                                        id="TaskInput"
                                        value="Strategic Planning"
                                        editable="false"
                                    />
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement label="Owner">
                                <f:fields>
                                    <ComboBox
                                        id="OwnerComboBox"
                                        selectedKey="John Doe"
                                    >
                                        <items>
                                            <core:Item
                                                key="John Doe"
                                                text="John Doe"
                                            />
                                            <core:Item
                                                key="Owner 1"
                                                text="Owner 1"
                                            />
                                            <core:Item
                                                key="Owner 2"
                                                text="Owner 2"
                                            />
                                        </items>
                                    </ComboBox>
                                </f:fields>
                            </f:FormElement>
 
                            <f:FormElement label="Start Date">
                                <f:fields>
                                    <DatePicker
                                        
                                        value="2024-01-1"
                                    />
                                </f:fields>
                            </f:FormElement>

                            <f:FormElement label="End Date">
                                <f:fields>
                                    <DatePicker
                                        
                                        value="2024-01-10"
                                    />
                                </f:fields>
                            </f:FormElement>
                        </f:formElements>
                    </f:FormContainer>
                </f:formContainers>
            </f:Form>
        </content>
        <footer>
            <Toolbar>
                <ToolbarSpacer />
                <Button
                    text="Save"
                     icon="sap-icon://save"
                    type="Emphasized"
                    press="onSave"
                />
                <Button
                    text="Delete"
                    icon="sap-icon://delete"
                    press=""
                    type="Reject"
                />
                <Button
                    text="Cancel"
                    press="onCancelEditDialog"
                />
            </Toolbar>
        </footer>
        </Dialog>
</core:FragmentDefinition>