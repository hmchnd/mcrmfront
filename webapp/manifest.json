{
  "_version": "1.65.0",
  "sap.app": {
    "id": "micro.crm.frontend",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.16.1",
      "toolsId": "0dec9ac8-1a97-4dcb-81d8-db4a4e6b8afb"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v2/framsys-api",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "framsys-manage": {
          "semanticObject": "framsys",
          "action": "manage",
          "title": "{{flpTitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "fullWidth": true
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.131.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f":{}
      }
    },
    "contentDensities": {
      "compact": false,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "micro.crm.frontend.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
            "useBatch": true
            
        }
    }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        },
        {
          "uri": "css/sapIllus-Fills.css"
        },
        {
          "uri": "css/sapIllus-Layout.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "micro.crm.frontend.view",
        "async": true,
        "viewPath": "micro.crm.frontend.view"
      },
      "routes": [
        {
          "name": "Dashboard",
          "pattern": "",
          "target":"Dashboard"
        },
        {
          "name": "manage_customers",
          "pattern": "manage_customers",
          "target": [
            "manage_customers"
          ]
        },
        {
          "name": "manage_products",
          "pattern": "manage_products",
          "target": [
            "manage_products"
          ]
        },
        {
          "name": "manage_leads",
          "pattern": "manage_leads",
          "target": [
            "manage_leads"
          ]
        }
        
      ],
      "targets": {
        "Dashboard":{
          "id":"Dashboard",
          "name": "Dashboard"
        },
        "manage_customers": {
          "id": "manage_customers",
          "name": "ManageCustomer"
        },
        "manage_products": {
          "id": "manage_products",
          "name": "ManageProducts"
        },
        "manage_leads": {
          "id": "manage_leads",
          "name": "ManageLeads"
        }
       
      }
    },
    "rootView": {
      "viewName": "micro.crm.frontend.view.App",
      "type": "XML",
      "id": "App"
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "project1"
  }
}